{
  "title": "Cama Autocomplete",
  "descr": "This is the plugin built with Camaleon CMS",
  "key": "cama_autocomplete", // must be the name of the folder of your plugin, sample: app/views/plugins/<my_plugin> ==> 'my_plugin'
  "helpers": [
    "Plugins::CamaAutocomplete::MainHelper"
  ],
  "hooks": {
    "on_active": ["cama_autocomplete_on_active"],
    "on_inactive": ["cama_autocomplete_on_inactive"],
    "plugin_options": ["cama_autocomplete_on_plugin_options"]
    //here you can add all your hooks (read documentation)
  }
}